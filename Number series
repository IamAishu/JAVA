import java.util.Scanner;
public class Main {
    public static int fun(int n) {
        if (n % 2 == 0) {
            return (n * n) - 2;
        } else {
            return (n * n) - 1;
        }
    }
public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int n = scanner.nextInt();
        for (int i = 1; i <= n; i++) {
            System.out.print(fun(i) + " ");
        }
    }
}

